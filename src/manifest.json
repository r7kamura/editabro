{
  "name": "Editabro",
  "description": "Editor in new tab.",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "src/background.ts"
  },
  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  },
  "permissions": ["storage"]
}
